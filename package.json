{
  "name": "ecoone",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "check:all": "yarn check:tsc && yarn check:eslint && yarn check:prettier",
    "check:tsc": "tsc --noEmit",
    "check:eslint": "eslint '*/**/*.{js,ts,tsx}' .",
    "check:prettier": "prettier --check \"**/*.*\"",
    "fix": "yarn fix:eslint && yarn fix:prettier",
    "fix:eslint": "eslint --fix '**/*.*'",
    "fix:prettier": "prettier --write '**/*.*'",
    "start": "react-native start",
    "run:android:dev": "react-native run-android --appId com.ecoone.development",
    "run:android:staging": "ENVFILE=.env.staging react-native run-android --variant=stagingRelease",
    "run:android:demo": "ENVFILE=.env.demo react-native run-android --variant=demoRelease",
    "run:android:beta": "ENVFILE=.env.beta react-native run-android --variant=betaRelease",
    "run:ios:dev": "react-native run-ios",
    "run:ios:staging": "react-native run-ios --scheme StagingEcoone",
    "run:ios:demo": "react-native run-ios --scheme DemoEcoone",
    "build:android:demo": "cd android && ENVFILE=.env.demo ./gradlew assembleDemoRelease",
    "build:android:staging": "cd android && ENVFILE=.env.staging ./gradlew assembleStagingRelease",
    "build:android:production": "cd android && ENVFILE=.env.production ./gradlew assembleRelease",
    "build:android:productionRelease": "cd android && ENVFILE=.env.production ./gradlew bundleRelease",
    "releaseTest:pushNewBuild:android:staging": "cd android && fastlane android_firebase_distribution",
    "releaseTest:pushNewBuild:android:demo": "cd android && fastlane android_firebase_distribution demo:true",
    "releaseTest:pushNewBuild:android:beta": "cd android && fastlane android_firebase_distribution beta:true",
    "releaseTest:update:android:staging": "cd android && fastlane android_code_push mandatory:true",
    "releaseTest:update:android:demo": "cd android && fastlane android_code_push mandatory:true demo:true",
    "releaseTest:update:android:beta": "cd android && fastlane android_code_push mandatory:true beta:true",
    "releaseTest:pushNewBuild:ios:staging": "cd ios && fastlane ios_firebase_distribution",
    "releaseTest:pushNewBuild:ios:demo": "cd ios && fastlane ios_firebase_distribution demo:true",
    "releaseTest:pushNewBuild:ios:beta": "cd ios && fastlane ios_firebase_distribution beta:true",
    "releaseTest:update:ios:staging": "cd ios && fastlane ios_code_push mandatory:true",
    "releaseTest:update:ios:demo": "cd ios && fastlane ios_code_push mandatory:true demo:true",
    "releaseTest:update:ios:beta": "cd ios && fastlane ios_code_push mandatory:true beta:true",
    "releaseTest:pushNewBuild:staging": "yarn releaseTest:pushNewBuild:android:staging && yarn releaseTest:pushNewBuild:ios:staging",
    "releaseTest:pushNewBuild:demo": "yarn releaseTest:pushNewBuild:android:demo && yarn releaseTest:pushNewBuild:ios:demo",
    "releaseTest:pushNewBuild:beta": "yarn releaseTest:pushNewBuild:android:beta && yarn releaseTest:pushNewBuild:ios:beta",
    "releaseTest:update:staging": "yarn releaseTest:update:android:staging && yarn releaseTest:update:ios:staging",
    "releaseTest:update:demo": "yarn releaseTest:update:android:demo && yarn releaseTest:update:ios:demo",
    "releaseTest:update:beta": "yarn releaseTest:update:android:beta && yarn releaseTest:update:ios:beta",
    "releaseBeta:pushNewbuild:android": "cd android && fastlane android_upload_to_play_store_internal_testing",
    "releaseBeta:pushNewbuild:ios": "cd ios && fastlane ios_upload_to_testflight_internal_testing",
    "releaseProduction:update:android": "cd android && fastlane android_code_push_production mandatory:true",
    "releaseProduction:update:ios": "cd ios && fastlane ios_code_push_production mandatory:true",
    "releaseProduction:update:all": "yarn releaseProduction:update:android && yarn releaseProduction:update:ios"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^1.0.0",
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/cameraroll": "^4.0.1",
